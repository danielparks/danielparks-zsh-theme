create_cd_repo cherry-pick-unstaged
change_files_then_commit a b
git checkout --quiet -b branch
change_files_then_commit a
git switch --quiet master
change_files_then_commit a
git cherry-pick branch &>/dev/null || true
date >a
git add a
git restore --staged a

